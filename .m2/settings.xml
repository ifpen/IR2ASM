<settings>
    <mirrors>
        <mirror>
            <id>internal-ifpen-repository</id>
            <name>IFPEN Internal Repo</name>
            <url>https://repos.ifpen.fr/repository/maven-central/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>gitlab-maven</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Job-Token</name>
                        <value>${CI_JOB_TOKEN}</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>
</settings>
